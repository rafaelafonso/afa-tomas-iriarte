{
  "name": "ampa",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios --device",
    "web": "expo start --web",
    "ios:sim": "expo start --ios --device 'iPhone 17 Pro'",
    "android:pixel": "expo start --android --device 'Pixel_8_Pro_API_34'",
    "stop": "lsof -ti:8081 | xargs kill -9 2>/dev/null || echo 'No hay servidor corriendo'",
    "stop:android": "~/Library/Android/sdk/platform-tools/adb devices | grep emulator | awk '{print $1}' | xargs -I {} ~/Library/Android/sdk/platform-tools/adb -s {} emu kill 2>/dev/null || echo 'No hay emulador corriendo'",
    "clean": "rm -rf node_modules && rm -rf .expo && npm install"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.12.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "expo": "~54.0.33",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
